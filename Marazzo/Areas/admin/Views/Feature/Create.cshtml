@model Feature
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/admin/Views/Shared/_Layout.cshtml";
}


<form asp-area="admin" asp-controller="Feature" asp-action="create" method="post" enctype="multipart/form-data">
    <div class="form-group">

        <label asp-for="@Model.Title">Title</label>
        <input class="form-control" asp-for="@Model.Title">
        <label asp-for="@Model.Icon">Setting icon</label>
        <input class="form-control" asp-for="@Model.Icon">
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
</form>

