@model Setting
@{
    Layout = "~/Areas/admin/Views/Shared/_Layout.cshtml";
}
<form asp-area="admin" asp-controller="Setting" asp-action="create" method="post" enctype="multipart/form-data">
    <div class="form-group">

        <label asp-for="@Model.Text">Feature title</label>
        <input class="form-control" asp-for="@Model.Text">
        <label asp-for="@Model.Icon">Feature icon</label>
        <input class="form-control" asp-for="@Model.Icon">
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
</form>


